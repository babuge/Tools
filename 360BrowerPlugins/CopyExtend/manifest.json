{
    "manifest_version": 3,
    "name": "CopyExtend",
    "description": "复制到剪贴板，防止被阻止。选中文本打开插件弹窗，即可粘贴。",
    "version": "1.0",
    "action": {
        "default_icon": "icon.png",
        "default_title": "复制",
        "default_popup": "popup.html"
    },
    "permissions": [
        "tabs",
        "scripting"
    ],
    "host_permissions": [
        "<all_urls>"
    ]
}